{
    "version": 2,
    "builds": [
       { "src": "./index.js", "use": "@vercel/node" },
       { "src": "./mymodule.py", "use": "@vercel/python" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/" }
    ],
    "functions": {
      "mymodule": {
        "runtime": "vercel-python-3.9"
      }
    }
 }